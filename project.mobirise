0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper\">\n            <div class=\"row justify-content-center\">\n                <div class=\"card col-sm-12 col-md-6 col-lg-3\">\n                    <h3 class=\"mbr-fonts-style mbr-title mbr-bold\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title\">CONTACTS</h3>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">\n                        +1234567890\n                    </p>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText &amp;&amp; textCount>1\">\n                        +1234567890\n                    </p>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText &amp;&amp; textCount>2\">\n                        +1234567890\n                    </p>\n                </div>\n                <div class=\"card col-sm-12 col-md-6 col-lg-3\" mbr-if=\"columns>1\">\n                    <h3 class=\"mbr-fonts-style mbr-title mbr-bold\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title\">COOPERATION</h3>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">\n                        info@site.com\n                    </p>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText &amp;&amp; textCount>1\">\n                        info@site.com\n                    </p>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText &amp;&amp; textCount>2\">\n                        info@site.com\n                    </p>\n                </div>\n                <div class=\"card col-sm-12 col-md-6 col-lg-3\" mbr-if=\"columns>2\">\n                    <h3 class=\"mbr-fonts-style mbr-title mbr-bold\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title\">CONTACTS</h3>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">\n                        +1234567890\n                    </p>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText &amp;&amp; textCount>1\">\n                        +1234567890\n                    </p>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText &amp;&amp; textCount>2\">\n                        +1234567890\n                    </p>\n                </div>\n                <div class=\"card col-sm-12 col-md-6 col-lg-3\" mbr-if=\"columns>3\">\n                    <h3 class=\"mbr-fonts-style mbr-title mbr-bold\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title\">COOPERATION</h3>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">\n                        info@site.com\n                    </p>\n                    <p class=\"mbr-fonts-style mbr-text mb-1 text\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText &amp;&amp; textCount>1\">\n         